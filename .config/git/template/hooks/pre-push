echo
echo "prek pre-push"
echo

HERE="$(cd "$(dirname "$0")" && pwd)"
PREK="/Users/tom/.local/share/mise/shims/prek"

# Check if the full path to prek is executable, otherwise fallback to PATH
if [ ! -x "$PREK" ]; then
    PREK="prek"
fi

exec "$PREK" hook-impl --hook-dir "$HERE" --script-version 4 --hook-type=pre-push --skip-on-missing-config -- "$@"

